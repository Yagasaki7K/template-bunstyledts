{
	"name": "template",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev --turbopack",
		"vercel": "next build",
		"start": "next start",
		"lint": "next lint",
		"create:mylint": "bunx --bun biome init && bun mylint",
		"mylint": "bunx biome format --write",
		"commit": "git pull && bun run vercel && bun mylint && read -p 'Descriptions Commit: ' message && git add . && echo -n \"$message - \" && node getTime.js | xargs -I {} git commit -m \"$message - {}\" && git push",
		"clean": "rm -rf node_modules && bun install"
	},
	"dependencies": {
		"@supabase/supabase-js": "^2.54.0",
		"next": "15.4.6",
		"next-seo": "^6.8.0",
		"react": "19.1.0",
		"react-dom": "19.1.0",
		"sonner": "^2.0.7",
		"styled-components": "^6.1.19",
		"uuid": "^11.1.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.1.1",
		"typescript": "^5",
		"@types/node": "^20",
		"@types/react": "^19",
		"@types/react-dom": "^19"
	}
}
